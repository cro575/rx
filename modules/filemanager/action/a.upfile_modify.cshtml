    //////////////////////////////////////////////////////////////////
    /// Get Request
    //////////////////////////////////////////////////////////////////
    var folder = Request["folder"];
    var oldfile = Request["oldfile"];
    var fileext = Request["fileext"];
    var alert = Request["alert"];
    var history = Request["history"];
    var uploadedFile = Request.Files[0];
    
    @_System.checkAdmin(my["admin"], 0)    
    
    if (uploadedFile != null)
    {
        string[] upFile_A = Path.GetFileName(uploadedFile.FileName).Split('.');
        string upFile_E = upFile_A[upFile_A.Length - 1].ToLower();
        
        if (upFile_E == fileext)
        {
            uploadedFile.SaveAs(folder + oldfile);
        }
    }
    
    @_System.getLink("reload", "parent.", alert, history)
    return;    
}