    //////////////////////////////////////////////////////////////////
    /// Get Request
    //////////////////////////////////////////////////////////////////
    var Overwrite = Request["Overwrite"];
    var upload_dir = Request["upload_dir"];
    var sess_Code = Request["sess_Code"];
    var uploadedFile = Request.Files[0];
       
    @_System.checkAdmin(my["admin"], 0)
    
    string savePath = upload_dir;
    string saveFile = savePath + Path.GetFileName(uploadedFile.FileName);
    string fileExt = _System.getExt(saveFile).ToLower();    
       
    if (Overwrite == "true" || File.Exists(saveFile))
    {
        if ("php3,html,inc,cgi,pl,jsp".Contains(fileExt))
        {
            return;
        }
                
        uploadedFile.SaveAs(saveFile);
    }
    
    return;
}