    var _HS = PageData[0];
    var my = PageData[1];
    var _CA = PageData[2];
    var s = PageData[3];
    var d_layout = PageData[4];
    var g = PageData[5];
    var keyword = PageData[6];
    var p = PageData[7];
    var system = PageData[8];
    var _m = PageData[9];
    var c = PageData[10];
    var m = PageData[11];
    var _mod = PageData[12];
    var _keyword = PageData[13];
    var _HM = PageData[14];
    var r = PageData[15];
    var bid = PageData[16];
    var _HP = PageData[17];
    var date = PageData[18];
    var referer = PageData[19];

    string prelayout = Request["prelayout"];
    
    dynamic LAYOUT_ARR = _DB.Get_code_layout(d_layout["dir"]);
    
    if(prelayout != "" && prelayout != null)
    {
        if(my["admin"] == "")
        {
            @_System.getLink(_System.RW(g["s"], g["r"], _HS), "", "권한이 없습니다.", "")
            return;
        }
        else
        {
            Session["g_main"] = g["main"];
        }        
    }

    //////////////////////////////////////////////////////////
    ///// Top File Rendering
    //////////////////////////////////////////////////////////
    @RenderPage("../../" + g["path_layout"] + d_layout["dir"] + "/_cross/top.cshtml", g, my, r, s, _HP, _CA, _HS, d_layout, LAYOUT_ARR, _HM, system)

    <div id="content">
    <div class="contentbody">
        @RenderPage("../../" + g["path_core"] + "engine/content.engine.cshtml", g, system, _m, c, m, p, _HS, my, d_layout, s, _keyword, _mod, r, bid, _HP, _HM, date, referer)
    </div>
    </div>
    
    //////////////////////////////////////////////////////////
    ///// Bottom File Rendering
    //////////////////////////////////////////////////////////
    @RenderPage("../../layouts/" + d_layout["dir"] + "/_cross/bottom.cshtml", g, r, _HS, _HM, system);
}
